@Article{DW.05,
  Title                    = {Distillation of secret key and entanglement from quantum states},
  Author                   = {Devetak, Igor and Winter, Andreas},
  Journal                  = {Proceedings of the Royal Society of London A: Mathematical, Physical and Engineering Sciences},
  Year                     = {2005},
  Number                   = {2053},
  Pages                    = {207--235},
  Volume                   = {461},

  Abstract                 = {We study and solve the problem of distilling a secret key from quantum states representing correlation between two parties (Alice and Bob) and an eavesdropper (Eve) via one{\textendash}way public discussion: we prove a coding theorem to achieve the {\textquoteleft}wire{\textendash}tapper{\textquoteright} bound, the difference of the mutual information Alice{\textendash}Bob and that of Alice{\textendash}Eve, for so{\textendash}called classical{\textendash}quantum{\textendash}quantum{\textendash}correlations, via one{\textendash}way public communication. This result yields information{\textendash}theoretic formulae for the distillable secret key, giving {\textquoteleft}ultimate{\textquoteright} key rate bounds if Eve is assumed to possess a purification of Alice and Bob{\textquoteright}s joint state. Specializing our protocol somewhat and making it coherent leads us to a protocol of entanglement distillation via one{\textendash}way LOCC (local operations and classical communication) which is asymptotically optimal: in fact we prove the so{\textendash}called {\textquoteleft}hashing inequality{\textquoteright}, which says that the coherent information (i.e. the negative conditional von Neumann entropy) is an achievable Einstein{\textendash}Podolsky{\textendash}Rosen rate. This result is known to imply a whole set of distillation and capacity formulae, which we briefly review.},
  Doi                      = {10.1098/rspa.2004.1372},
  Eprint                   = {http://rspa.royalsocietypublishing.org/content/461/2053/207.full.pdf},
  ISSN                     = {1364-5021},
  Publisher                = {The Royal Society},
  Url                      = {http://rspa.royalsocietypublishing.org/content/461/2053/207}
}

